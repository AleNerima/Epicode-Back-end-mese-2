@model DittaSpedizioniApp.Models.Cliente

@{
    ViewData["Title"] = "Dettagli Cliente";
}

<h2>@ViewData["Title"]</h2>

<div>
    <h4>Cliente</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Nome</dt>
        <dd class="col-sm-10">@Model.Nome</dd>
        <dt class="col-sm-2">Tipo</dt>
        <dd class="col-sm-10">@Model.Tipo</dd>
        <dt class="col-sm-2">Codice Fiscale</dt>
        <dd class="col-sm-10">@Model.CodiceFiscale</dd>
        <dt class="col-sm-2">Partita IVA</dt>
        <dd class="col-sm-10">@Model.PartitaIVA</dd>
    </dl>
    <a href="/Cliente" class="btn btn-secondary">Indietro</a>
</div>
